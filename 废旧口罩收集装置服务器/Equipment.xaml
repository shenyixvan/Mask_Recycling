                <GroupBox
                    Header="          箱内湿度历史曲线" FontSize="18" Margin="530,10,310,435"/>
                <GroupBox
                    Header="         臭氧浓度历史曲线" FontSize="18" Margin="812,252,28,193"/>
                <GroupBox
                    Header="       紫外线强度历史曲线" FontSize="18" Margin="812,10,28,435"/>
                <GroupBox
                    Header="        箱内压强历史曲线" FontSize="18" Margin="250,252,590,193"/>
