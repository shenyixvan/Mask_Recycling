                        <TextBlock x:Name="TextBlock_History" Margin="0,10,0,11" Width="153" Text="" FontSize="14"/>
                    </GroupBox>
                </Grid>
                <Grid HorizontalAlignment="Left" Height="194" Margin="250,51,0,0" VerticalAlignment="Top" Width="260" Background="AntiqueWhite">
